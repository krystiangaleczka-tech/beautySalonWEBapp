{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020"],
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true
  },
  "include": ["admin-frontend/**/*"],
  "exclude": ["node_modules", "booking-functions", "**/lib", "**/*.spec.ts", "**/*.test.ts"]
}